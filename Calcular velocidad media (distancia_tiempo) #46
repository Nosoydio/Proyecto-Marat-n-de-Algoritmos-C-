#include <iostream>
using namespace std;

int main() {
    double distancia, tiempo, velocidad;
    cout << "Ingrese la distancia recorrida (en metros): ";
    cin >> distancia;
    cout << "Ingrese el tiempo empleado (en segundos): ";
    cin >> tiempo;
    velocidad = distancia / tiempo;
    cout << "La velocidad media es: " << velocidad << " m/s." << endl;
    return 0;
}
