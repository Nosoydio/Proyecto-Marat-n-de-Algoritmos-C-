#include <iostream>
using namespace std;

int main() {
    double distancia, velocidad, tiempo;
    cout << "Ingrese la distancia (en metros): ";
    cin >> distancia;
    cout << "Ingrese la velocidad (en m/s): ";
    cin >> velocidad;
    tiempo = distancia / velocidad;
    cout << "El tiempo necesario es: " << tiempo << " segundos." << endl;
    return 0;
}
