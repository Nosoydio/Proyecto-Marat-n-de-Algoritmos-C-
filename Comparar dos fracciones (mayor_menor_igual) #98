#include <iostream>
using namespace std;

int main() {
    int num1, den1, num2, den2;
    cout << "Ingrese numerador y denominador de la primera fracci贸n (ej. 3 4): ";
    cin >> num1 >> den1;
    cout << "Ingrese numerador y denominador de la segunda fracci贸n (ej. 5 6): ";
    cin >> num2 >> den2;

    if (den1 == 0 || den2 == 0) {
        cout << "Error: el denominador no puede ser cero." << endl;
        return 0;
    }

    int izquierda = num1 * den2;
    int derecha = num2 * den1;

    if (izquierda > derecha) {
        cout << "La primera fracci贸n es mayor que la segunda." << endl;
    } else if (izquierda < derecha) {
        cout << "La primera fracci贸n es menor que la segunda." << endl;
    } else {
        cout << "Ambas fracciones son iguales." << endl;
    }

    return 0;
}
