#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de elementos del arreglo ordenado: ";
    cin >> n;
    int arreglo[n];
    for (int i = 0; i < n; i++) {
        cout << "Elemento " << i + 1 << ": ";
        cin >> arreglo[i];
    }

    int nuevo_tamano = 1;
    for (int i = 1; i < n; i++) {
        if (arreglo[i] != arreglo[nuevo_tamano - 1]) {
            arreglo[nuevo_tamano] = arreglo[i];
            nuevo_tamano++;
        }
    }

    cout << "Arreglo sin duplicados consecutivos: ";
    for (int i = 0; i < nuevo_tamano; i++) {
        cout << arreglo[i] << " ";
    }
    cout << endl;
    return 0;
}
