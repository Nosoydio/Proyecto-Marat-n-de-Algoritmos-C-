#include <iostream>
using namespace std;

int main() {
    int n, contador = 0;
    cout << "Ingrese la cantidad de elementos del arreglo: ";
    cin >> n;
    double arreglo[n];
    for (int i = 0; i < n; i++) {
        cout << "Ingrese el elemento " << i + 1 << ": ";
        cin >> arreglo[i];
        if (arreglo[i] > 0) {
            contador++;
        }
    }
    cout << "Cantidad de elementos positivos: " << contador << endl;
    return 0;
}
