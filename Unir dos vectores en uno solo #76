#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n1, n2;
    cout << "Ingrese la cantidad de elementos del primer vector: ";
    cin >> n1;
    vector<int> v1(n1);
    for (int i = 0; i < n1; i++) {
        cout << "v1[" << i << "]: ";
        cin >> v1[i];
    }

    cout << "Ingrese la cantidad de elementos del segundo vector: ";
    cin >> n2;
    vector<int> v2(n2);
    for (int i = 0; i < n2; i++) {
        cout << "v2[" << i << "]: ";
        cin >> v2[i];
    }

    vector<int> combinado = v1;
    combinado.insert(combinado.end(), v2.begin(), v2.end());

    cout << "Vector combinado: ";
    for (int x : combinado) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}
