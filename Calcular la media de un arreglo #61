#include <iostream>
using namespace std;

int main() {
    int n;
    double suma = 0;
    cout << "Ingrese la cantidad de elementos del arreglo: ";
    cin >> n;
    double arreglo[n];
    for (int i = 0; i < n; i++) {
        cout << "Ingrese el elemento " << i + 1 << ": ";
        cin >> arreglo[i];
        suma += arreglo[i];
    }
    double media = suma / n;
    cout << "La media de los elementos es: " << media << endl;
    return 0;
}
