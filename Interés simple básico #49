#include <iostream>
using namespace std;

int main() {
    double capital, tasa, tiempo, interes;
    cout << "Ingrese el capital inicial: ";
    cin >> capital;
    cout << "Ingrese la tasa de interés anual (en porcentaje): ";
    cin >> tasa;
    cout << "Ingrese el tiempo en años: ";
    cin >> tiempo;
    interes = capital * (tasa / 100) * tiempo;
    cout << "El interés simple generado es: " << interes << endl;
    return 0;
}
