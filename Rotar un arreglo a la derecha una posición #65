#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de elementos del arreglo: ";
    cin >> n;
    int arreglo[n];
    for (int i = 0; i < n; i++) {
        cout << "Elemento " << i + 1 << ": ";
        cin >> arreglo[i];
    }

    int ultimo = arreglo[n - 1];
    for (int i = n - 1; i > 0; i--) {
        arreglo[i] = arreglo[i - 1];
    }
    arreglo[0] = ultimo;

    cout << "Arreglo rotado a la derecha: ";
    for (int i = 0; i < n; i++) {
        cout << arreglo[i] << " ";
    }
    cout << endl;
    return 0;
}
